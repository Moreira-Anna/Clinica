	<parameter name="Cpf" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT p.nome,
	c.datacon,
	c.descricao AS nome_paciente,
	COALESCE( m.nome,'Clinico Geral') AS nome_medico
FROM consulta c
	JOIN paciente p ON 
	 c.cpf_paciente = p.cpf 
	LEFT JOIN medico m ON 
	 c.cpf_paciente = m.cfm 
WHERE 
	 p.cpf = $P{Cpf}]]>
	</queryString>
